{
  "tenantId": "nexus-bank-prod",
  "tenantName": "Nexus Bank - Production",
  "tenantImage": "/assets/tenants/nexus-bank/logo.png",
  "customer": "nexus-bank",
  "environment": "production",
  "api": {
    "baseUrl": "https://api.nexusbank.io/v1",
    "authUrl": "https://auth.nexusbank.io/oauth2/token",
    "timeout": 20000,
    "retryAttempts": 5
  },
  "theme": {
    "primaryColor": "#059669",
    "secondaryColor": "#10b981",
    "fontFamily": "Inter, sans-serif"
  },
  "features": {
    "enabledModules": ["reporting", "analytics", "audit-log", "notifications", "user-management"],
    "customSettings": {
      "debugMode": false,
      "logLevel": "error",
      "enableTestFeatures": false,
      "mockPayments": false,
      "kycValidation": "strict",
      "highAvailability": true,
      "backupFrequency": "hourly",
      "fraudDetection": true,
      "complianceMode": "pci-dss"
    }
  }
}
